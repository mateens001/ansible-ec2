# EC2 instance variable

ec2:
  region: eu-west-1
  credentials: test_key
  image: ami-1c887d65
  sg: default
  instance_type: t2.micro
  subnet: subnet-fd361cb4
  public_ip: yes
  tags:
    Name: Ec2Ansible
  volumes:
    - device_name: /dev/xvda
      volume_type: standard
      volume_size: 8
  



